{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "app",
      "image": "vivanou/multi-service",
      "hostname": "app",
      "essential": false,
      "memory": 256
    },
    {
      "name": "nginx",
      "image": "vivanou/multi-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "memory": 128,
      "links": ["app"],
      "dependsOn": [
        {
          "containerName": "app",
          "condition": "SUCCESS"
        }
      ]
    }
  ]
}