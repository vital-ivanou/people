{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "app",
      "image": "vivanou/multi-service",
      "hostname": "app",
      "essential": false,
      "memory": 128,
      "links": ["cache", "db"]
    },
    {
      "name": "cache",
      "image": "redis:latest",
      "hostname": "cache",
      "essential": false,
      "memory": 128
    },
    {
      "name": "db",
      "image": "mysql:5.7",
      "hostname": "db",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "vivanou/multi-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "memory": 128,
      "links": ["app"]
    }
  ]
}