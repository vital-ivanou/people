{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "app",
      "image": "vivanou/multi-service",
      "hostname": "app",
      "essential": false
    },
    {
      "name": "cache",
      "image": "redis:latest",
      "hostname": "cache",
      "essential": false
    },
    {
      "name": "db",
      "image": "mysql:5.7",
      "hostname": "db",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "vivanou/multi-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    }
  ]
}