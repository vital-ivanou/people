{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "app",
      "image": "vivanou/multi-service",
      "hostname": "app",
      "essential": true,
      "memory": 256,
      "links": ["cache", "db"],
      "dependsOn": [
        {
          "containerName": "db",
          "condition": "SUCCESS "
        },
        {
          "containerName": "cache",
          "condition": "SUCCESS "
        }
      ]
    },
    {
      "name": "cache",
      "image": "redis:latest",
      "hostname": "cache",
      "essential": true,
      "memory": 128
    },
    {
      "name": "db",
      "image": "mysql:5.7",
      "hostname": "db",
      "essential": true,
      "memory": 512
    },
    {
      "name": "nginx",
      "image": "vivanou/multi-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "memory": 128,
      "links": ["app"],
      "dependsOn": [
        {
          "containerName": "app",
          "condition": "SUCCESS "
        }
      ]
    }
  ]
}